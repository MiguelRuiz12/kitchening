<!DOCTYPE html>
<html lang="ES">

<%- include('./partials/head',{
	title : "Kitchening"
}) %>

<body class="home">

	<%- include('./partials/header') %>
		
		<main>
			<!-- banner -->
			<div>
				<img class="home__main__banner" src="images/img-banner.jpg" alt="banner">
			</div>

			<!-- productos -->
			<div class="home__main__products">

				<% products.forEach(product => { %>
					<%- include('./partials/product-card',{
						...product
					}) %>

				<% }) %>
				
			</div>
		</main>
		<%- include('./partials/footer') %>
</body>

</html>